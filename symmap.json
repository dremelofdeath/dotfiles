{
  "vim": {
    "files": [
      "vimfiles",
      ".vimrc",
      "..vimrc.un~"
    ]
  },
  "screen": {
    "platform": "unix",
    "file": ".screenrc"
  },
  "ConEmu": {
    "platform": "windows",
    "file": {
      "from": "ConEmu.xml",
      "to": "AppData\\Roaming\\ConEmu.xml"
    }
  },
  "bash": {
    "platform": "unix",
    "files": [
      ".bash_profile",
      ".bashrc"
    ]
  },
  "fonts": {
    "platform": "unix",
    "file": {
      "from": "fonts",
      "to": ".fonts"
    }
  }
}
